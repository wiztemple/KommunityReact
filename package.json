{
  "name": "KommunityReact",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --open --hot",
    "test": "jest --updateSnapshot --coverage",
    "lint": "eslint --init --fix",
    "start": "npm run build && node dist/server.js",
    "build": "webpack --config webpack.config.prod.js && babel server.js --out-file dist/server.js"
  },
  "repository": "https://github.com/wiztemple/KommunityReact.git",
  "author": "wiztemple <wiztemple@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.3",
    "history": "^4.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@testing-library/react": "^8.0.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "selenium-server": "^3.141.59",
    "selenium-standalone": "^6.16.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "./*/nightwatch.js",
      "public",
      "./*/store.js",
      "./*/mockData.js"
    ]
  }
}
